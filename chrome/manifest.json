{
  "name": "AWS Console Fixes",
  "version": "1.0.2",
  "description": "Fix annoyances in the AWS console.",
  "homepage_url": "https://gitlab.com/stefansundin/aws-console-fixes",
  "author": "Stefan Sundin",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.console.aws.amazon.com/*",
        "https://forums.aws.amazon.com/*"
      ],
      "css": ["style.css"]
    }, {
      "matches": ["https://*.es.amazonaws.com/_plugin/kibana/*"],
      "css": ["kibana.css"]
    }, {
      "matches": ["https://aws.amazon.com/*"],
      "js": ["whitepaper.js"]
    }, {
      "matches": ["https://*.console.aws.amazon.com/ec2/v2/home*"],
      "js": ["ec2.js"]
    }, {
      "matches": ["https://*.console.aws.amazon.com/cloudformation/home*"],
      "js": ["cloudformation.js"]
    }, {
      "matches": ["https://*.console.aws.amazon.com/sns/v2/home*"],
      "js": ["sns.js"]
    }, {
      "matches": ["https://*.console.aws.amazon.com/iam/home*"],
      "js": ["iam.js"]
    }, {
      "matches": ["https://*.console.aws.amazon.com/cloudwatch/home*"],
      "js": ["cloudwatch.js"]
    }, {
      "matches": [
        "https://www.amazon.com/ap/signin*",
        "https://*.signin.aws.amazon.com/oauth*"
      ],
      "js": ["signin.js"]
    }
  ],
  "manifest_version": 2
}
